{
    "name": "mmft24/elastic-apm-symfony-bundle",
    "type": "symfony-bundle",
    "description": "Integrate Elastic APM into Symfony",
    "keywords": ["elastic", "apm", "monitoring", "performance", "metrics", "symfony", "symfony-bundle"],
    "homepage": "https://github.com/mmft24/elastic-apm-symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "symfony/config": "^6.0|^7.0",
        "symfony/console": "^6.0|^7.0",
        "symfony/dependency-injection": "^6.0|^7.0",
        "symfony/event-dispatcher": "^6.0|^7.0",
        "symfony/http-kernel": "^6.0|^7.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^6.2",
        "nyholm/symfony-bundle-test": "^3.1",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1",
        "rector/rector": "^2.2",
        "symfony/phpunit-bridge": "^8.0.0",
        "symplify/easy-coding-standard": "^13.0",
        "vimeo/psalm": "^6.13"
    },
    "autoload": {
        "psr-4": {
            "ElasticApmBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElasticApmBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": {
            "*": "dist"
        },
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
